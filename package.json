{
  "name": "fluidpay-tokenizer",
  "version": "1.15.0",
  "main": "tmp/tokenizer.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "repository": "https://github.com/fluidpay/fluidpay-tokenizer",
  "scripts": {
    "dev": "vite --config vite.config.docs.ts",
    "serve": "vite preview",
